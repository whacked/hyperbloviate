;; util
(macro m-transact! (...body)
       (^*scoped-without-return
        `(do
          (m.startComputation)
          ...@body
          (m.endComputation))))

(macro with-return-var (rtn init ...body)
       (^*scoped-without-return
        `(do
          (var @rtn @init)
          ...@body
          @rtn)))
